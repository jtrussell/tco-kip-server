{
  "name": "keyteki-private",
  "version": "1.0.0",
  "description": "A Keyforge Card Game Server",
  "main": "index.js",
  "scripts": {
    "mongod": "mongod --dbpath=/data --port 27017\n",
    "start": "NODE_ENV=development PORT=4000 POSTGRES_CONNECTION_STRING=postgres://postgres:postgres@127.0.0.1/postgres REDIS_URL=redis://localhost:4001 MONGODB_CONNECTION_STRING=mongodb://127.0.0.1:27017/keyforge node .",
    "psql": "docker exec -it tco-kip-server_postgres_1 psql postgres://postgres:postgres@127.0.0.1/postgres",
    "fetchdata": "node server/scripts/fetchdata",
    "game": "PORT=5000 POSTGRES_CONNECTION_STRING=postgres://postgres:postgres@127.0.0.1/postgres REDIS_URL=redis://localhost:4001 MONGODB_CONNECTION_STRING=mongodb://127.0.0.1:27017/keyforge node server/gamenode",
    "test": "cross-env JASMINE_CONFIG_PATH=./jasmine.json jasmine",
    "lint": "eslint --ext=js --ext=jsx client/ server/ test/",
    "b": "rm -fr dist/ && webpack --config webpack.production.js",
    "bb": "rm -fr dist/ && NODE_ENV=development webpack --config webpack.dev.js",
    "test-client": "jest --colors client"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./client/setup-tests.js"
    ]
  },
